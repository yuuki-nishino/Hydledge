    "@nuxtjs/pwa": "^3.3.5",
    "core-js": "^3.19.3",
    "fabric": "^5.2.1",
    "next-optimized-images": "^2.6.2",
    "nuxt": "^2.12.2",
    "svg-japan": "^1.1.2",
    "vue": "^2.6.14",
    "vue-google-charts": "^0.3.3",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vue2-google-maps": "^0.10.7",
    "vuetify": "^2.6.1"
  },
  "devDependencies": {
    "@aceforth/nuxt-optimized-images": "^1.4.0",
